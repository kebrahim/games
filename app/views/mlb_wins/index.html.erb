<h1>Listing mlb_wins</h1>

<table class='table table-striped table-bordered table-condensed'>
  <tr>
  	<th>Team</th>
    <th>Year</th>
    <th>Over/Under</th>
    <th>Actual</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mlb_wins.each do |mlb_win| %>
  <tr>
    <td><%= mlb_win.mlb_team.abbreviation %></td>
    <td><%= mlb_win.year %></td>
    <td><%= mlb_win.line %></td>
    <td><%= mlb_win.actual %></td>
    <td><%= link_to 'Show', mlb_win %></td>
    <td><%= link_to 'Edit', edit_mlb_win_path(mlb_win) %></td>
    <td><%= link_to 'Destroy', mlb_win, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mlb win', new_mlb_win_path %>
